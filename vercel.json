{
  "version": 2,
  "builds": [
    { "source": "api/index.js", "use": "@vercel/node" },
    { "source": "src/**/*", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/admin", "destination": "/src/admin/index.js" }

  ]
}
